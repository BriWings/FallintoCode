		<script>
			$(document).ready(function() {
				fetchOOS();
				$('button').button();
				$('.multib').button({
					icons: {
						primary: "ui-icon-triangle-1-s"
					}
				});
				$('.multib .ui-button-text').css({"padding-left":"0px","padding-right":"0px"});
				
				 $('#court').click(function() {
					$('#courtdialog').dialog('open'); 
				});
				
				$('#special').click(function() {
					$('#specialdialog').dialog('open');
				});
				
				 $('.oos').click(function(event) {
					var oosReason = $(event.target).text();
					$('#Reason').val(oosReason);
					//if ($('.ui-dialog').is(':visible')) {
					//	$('.ui-dialog').dialog('close');
					//}
					validatepage();
				});
				
				/* Select     */
			      $('#courtdialog').dialog({
					autoOpen: false,
					height: "400",
					width: "700",
					modal: true,
					position: {my: "center", at: "center"},
					title: "Choose Out Of Service / COURT", 
					draggable: false,
					show: 'fade',
					hide: 'fade',
					resizable: false
			
				});  
				
				 $('#specialdialog').dialog({
					autoOpen: false,
					height: "450",
					width: "700",
					modal: true,
					position: {my: "center", at: "center"},
					title: "Choose Out of Service / SPECIAL ASSIGNMENT ", 
					draggable: false,
					show: 'fade',
					hide: 'fade',
					resizable: false
				}); 
				
				 
			}); //Close Document Ready
			
			function validatepage() {
				oosForm.submit.click();
			} 
		</script>
